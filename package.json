{
  "name": "redux-global-loading",
  "version": "1.0.0",
  "description": "A Redux Middleware that show a Loading when wait resolve all promise-middleware's promises",
  "main": "lib/index.js",
  "scripts": {
    "babel": "./node_modules/.bin/babel src -d lib",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --require tests/setup.js tests/*.spec.js",
    "test:tdd": "npm t -- --watch",
    "test:coverage": "NODE_ENV=test ./node_modules/.bin/nyc --reporter=html --reporter=text ./node_modules/.bin/mocha tests/setup.js tests/*.spec.js",
    "lint": "./node_modules/.bin/eslint src",
    "lint:fix": "./node_modules/.bin/eslint src --fix",
    "prepush": "npm run lint && npm t"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guilouro/redux-global-loading.git"
  },
  "nyc": {
    "lines": 90,
    "check-coverage": true,
    "exclude": [
      "node_modules",
      "tests",
      "lib",
      "src/index.js"
    ]
  },
  "keywords": [
    "redux",
    "react",
    "middleware",
    "loading",
    "promises"
  ],
  "author": "Guilherme Louro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guilouro/redux-global-loading/issues"
  },
  "homepage": "https://github.com/guilouro/redux-global-loading#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.1",
    "coveralls": "^2.13.1",
    "enzyme": "^2.9.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.2.1",
    "husky": "^0.14.3",
    "jsdom": "^11.1.0",
    "mocha": "^3.5.0",
    "nyc": "^11.1.0",
    "react-test-renderer": "^15.6.1",
    "sinon": "^3.2.1",
    "sinon-chai": "^2.13.0"
  },
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "prop-types": "^15.5.10"
  }
}
